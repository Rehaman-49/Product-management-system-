package arraylist;
import java.util.ArrayList;  // Fixed: ArrayList (capital L), not Arraylist
import java.util.Collections;

public class ReverseArrayList {  // Fixed: PascalCase class name
    public static void main(String[] args) {  // Fixed: args (not string[]args)
        ArrayList<Integer> list = new ArrayList<>();  // Fixed: <> diamond operator

        list.add(10);
        list.add(20);
        list.add(30);
        list.add(40);

        System.out.println("Original list: " + list);

        Collections.reverse(list);

        System.out.println("Reversed list: " + list);
    }
}
